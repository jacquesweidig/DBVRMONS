<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - VR Citizens</title>
    <link href="~/images/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/images/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/scripts/modernizr-2.6.2.js"></script>
    <script src="~/scripts/coteGraph.js"></script>
</head>
<body>
    
    <div class="navbar navbar-inverse navbar-fixed-top" style="background-color : #ffffff">
        <div class="container">
            <div class="navbar-header">
                <img src="~/images/VR citizens logo.png" style="margin-top : 20px; margin-bottom : 20px;" />
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav"></ul>
            </div>
        </div>
    </div>
    
    <div class="container body-images" style="margin-top:120px;">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Hackaton</p>
        </footer>
    </div>

    <script src="~/scripts/jquery-1.9.1.min.js"></script>
    <script src="~/scripts/bootstrap.min.js"></script>
</body>
</html>
